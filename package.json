{
  "name": "copilotedge",
  "version": "0.2.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "description": "Adapter for connecting CopilotKit to Cloudflare Workers AI with caching and rate limiting",
  "keywords": [
    "copilotkit",
    "cloudflare",
    "workers-ai",
    "edge-computing",
    "llm",
    "adapter",
    "cache"
  ],
  "author": "Klammertime",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Klammertime/copilotedge.git"
  },
  "bugs": {
    "url": "https://github.com/Klammertime/copilotedge/issues"
  },
  "homepage": "https://github.com/Klammertime/copilotedge#readme",
  "scripts": {
    "lint": "eslint . --max-warnings=0",
    "build": "tsc",
    "test": "vitest run",
    "test:unit": "vitest run test/unit.test.ts",
    "verify": "./verify.sh",
    "prepublishOnly": "pnpm build && pnpm test:unit && pnpm verify",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:integration": "vitest run test/integration.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.25.0"
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.56.0",
    "miniflare": "^3.20250718.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
