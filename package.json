{
  "name": "copilotedge",
  "version": "0.2.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "description": "Adapter for connecting CopilotKit to Cloudflare Workers AI with caching and rate limiting",
  "keywords": [
    "copilotkit",
    "cloudflare",
    "workers-ai",
    "edge-computing",
    "llm",
    "adapter",
    "cache"
  ],
  "author": "Klammertime",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Klammertime/copilotedge.git"
  },
  "bugs": {
    "url": "https://github.com/Klammertime/copilotedge/issues"
  },
  "homepage": "https://github.com/Klammertime/copilotedge#readme",
  "scripts": {
    "build": "node node_modules/typescript/lib/_tsc.js",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:integration": "vitest run test/integration.test.ts",
    "lint": "eslint src test --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "miniflare": "^3.0.0",
    "typescript": "^5.9.2",
    "vitest": "^1.2.0"
  }
}
