{
  "name": "copilotedge",
  "version": "0.9.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./rate-limiter": {
      "types": "./dist/rate-limiter.d.ts",
      "import": "./dist/rate-limiter.js",
      "require": "./dist/rate-limiter.js"
    },
    "./security": {
      "types": "./dist/security.d.ts",
      "import": "./dist/security.js",
      "require": "./dist/security.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "description": "Bridge any AI frontend to Cloudflare Workers AI - Protocol adapter for CopilotKit and OpenAI with built-in telemetry",
  "keywords": [
    "cloudflare-workers-ai",
    "copilotkit-adapter",
    "openai-compatible",
    "edge-computing",
    "llm",
    "telemetry",
    "token-tracking",
    "cost-tracking",
    "workers-ai"
  ],
  "author": "Klammertime",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Klammertime/copilotedge.git"
  },
  "bugs": {
    "url": "https://github.com/Klammertime/copilotedge/issues"
  },
  "homepage": "https://github.com/Klammertime/copilotedge#readme",
  "scripts": {
    "lint": "eslint . --max-warnings=0",
    "build": "tsc",
    "test": "vitest run --config vitest.config.mjs",
    "test:unit": "vitest run --config vitest.config.mjs test/unit.test.ts",
    "verify": "./verify.sh",
    "prepublishOnly": "pnpm build && pnpm test:unit && pnpm verify",
    "test:coverage": "vitest run --config vitest.config.mjs --coverage",
    "test:watch": "vitest watch --config vitest.config.mjs",
    "test:integration": "vitest run --config vitest.config.mjs test/integration.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "js-tiktoken": "^1.0.21"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.25.0"
    }
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "miniflare": "^4.20250813.1",
    "next": "^15.4.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
